{
    "name": "integer-net/grumphp-todo-reminder",
    "description": "GrumPHP Extension to remind you of undone todos",
    "type": "library",
    "bin": ["bin/todo"],
    "require": {
        "php": "~7.1",
        "ext-standard": "*",
        "ext-mbstring": "*",
        "ext-SPL": "*",
        "gitonomy/gitlib": "^1.0",
        "nikic/php-parser": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "phpro/grumphp": "^0.13.1",
        "jakub-onderka/php-parallel-lint": "^0.9.2",
        "squizlabs/php_codesniffer": "^3.2",
        "phpstan/phpstan": "^0.9.2",
        "maglnet/composer-require-checker": "^0.2.0",
        "infection/infection": "^0.7.0",
        "phan/phan": "^0.11.1",
        "phpmd/phpmd": "^2.6",
        "pds/skeleton": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "IntegerNet\\TodoReminder\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IntegerNet\\TodoReminder\\": "tests/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Fabian Schmengler",
            "email": "fs@integer-net.de"
        }
    ],
    "minimum-stability": "stable"
}
